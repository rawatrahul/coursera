   "source": [
    "fname = input(\"Enter file name: \")\n",
    "fh = open(fname)\n",
    "lst = list()\n",
    "for line in fh:\n",
    "    #print(line.split(' '))\n",
    "    lst_line = line.split(' ')\n",
    "    for word in lst_line:\n",
    "        if word.strip() not in lst :\n",
    "            lst.append(word.strip())\n",
    "lst.sort()\n",
    "print(lst)"
   ]
  }
